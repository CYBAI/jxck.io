<meta name="author" content="Jxck">
    <meta name="description" content="<%= short hsp description %>">
    <meta name="keywords" content="<%= tags.join(",") %>">
    <meta itemprop="image" content="<%= icon %>">
    <meta itemprop="name" content="<%= host %>">

    <meta name="twitter:card" content="summary">
    <meta name="twitter:site" content="@jxck_">
    <meta name="twitter:url" content="<%= canonical %>">
    <meta name="twitter:title" content="<%= title %> | <%= host %>">
    <meta name="twitter:description" content="<%= short hsp description %>">
    <meta name="twitter:image" content="<%= icon %>">

    <meta property="og:type" content="article">
    <meta property="og:url" content="<%= canonical %>">
    <meta property="og:title" content="<%= title %> | <%= host %>">
    <meta property="og:site_name" content="<%= host %>">
    <meta property="og:description" content="<%= short hsp description %>">
    <meta property="og:image" content="<%= icon %>">

    <link rel="icon" sizes="192x192" href="<%= icon %>">
    <link rel="apple-touch-icon" sizes="128x128" href="<%= icon %>">
